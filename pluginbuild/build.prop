#文件路径要么是绝对路径, 要么是相对于工程根目录的相对路径

#签名设置
keyStoreFile = app/debug.keystore
keyStorePass = android
keyAlias = androiddebugkey
keyAliasPass = android

#公共资源模块, 该模块下的res目录会被打包到插件中
commResModule = resource
#是否混淆插件代码
doProguard = false

#是否copy插件到app assets目录
copyPluginToAppAssets = true

#onlyAssetInPluginDir - 是否只打包插件assets目录下plugin目录下的资源
#onlyCodeInPluginPackage, 是否只打包com.ljj.fragplugin.pluginName目录下的class, 若不是, 则打包src/main/java下所有代码
onlyAssetInPluginDir = false
onlyCodeInPluginPackage = false
